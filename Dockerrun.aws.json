{
    "AWSEBDockerrunVersion": "3",
    "containerDefinitions": [
      {
        "name": "bokted-backend",
        "image": "597268614173.dkr.ecr.us-east-1.amazonaws.com/bokted-backend",
        "memory": 128,
        "portMappings": [
          {
            "hostPort": 3080,
            "containerPort": 3080
          }
        ], 
        "ports": [
          {
            "hostPort": 3080,
            "containerPort": 3080
          }
        ],
        "essential": true
      },
      {
        "name": "bokted-frontend",
        "image": "597268614173.dkr.ecr.us-east-1.amazonaws.com/bokted-frontend",
        "memory": 128,
        "portMappings": [
          {
            "hostPort": 3000,
            "containerPort": 3000
          }
        ],
        "ports": [
          {
            "hostPort": 3000,
            "containerPort": 3000
          }
        ],
        "essential": true
      },
      {
        "name": "bokted-nginx",
        "image": "597268614173.dkr.ecr.us-east-1.amazonaws.com/bokted-nginx",
        "memory": 128,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort": 80
          }
        ],
        "ports": [
          {
            "hostPort": 80,
            "containerPort": 80
          }
        ],
        "links": ["bokted-backend", "bokted-frontend"],
        "essential": true
      }
    ]
  }