{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "backend",
      "image": "597268614173.dkr.ecr.us-east-1.amazonaws.com/bokted-backend:latest",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "containerPort": 3080,
          "hostPort": 3080
        }
      ]
    },
    {
      "name": "frontend",
      "image": "597268614173.dkr.ecr.us-east-1.amazonaws.com/bokted-frontend:latest",
      "essential": true,
      "memory": 256,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000
        }
      ]
    },
    {
      "name": "nginx",
      "image": "597268614173.dkr.ecr.us-east-1.amazonaws.com/bokted-nginx:latest",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ],
      "links": [
        "backend",
        "frontend"
      ]
    }
  ]
}
  