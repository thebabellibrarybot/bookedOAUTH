{
  "AWSEBDockerrunVersion": "1",
  "containerDefinitions": [
    {
      "name": "bokted-frontend",
      "image": "597268614173.dkr.ecr.us-east-1.amazonaws.com/bokted-frontend:latest",
      "memory": 128,
      "Ports": [
        {
          "hostPort": 3000,
          "containerPort": 3000
        }
      ],
      "essential": true
    },
    {
      "name": "bokted-backend",
      "image": "597268614173.dkr.ecr.us-east-1.amazonaws.com/bokted-backend:latest",
      "memory": 128,
      "Ports": [
        {
          "hostPort": 3080,
          "containerPort": 3080
        }
      ],
      "essential": true
    },
    {
      "name": "bokted-nginx",
      "image": "597268614173.dkr.ecr.us-east-1.amazonaws.com/bokted-nginx:latest",
      "memory": 128,
      "Ports": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["bokted-backend", "bokted-frontend"],
      "essential": true
    }
  ]
}
